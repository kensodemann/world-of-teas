{
  "name": "world-of-teas",
  "version": "0.0.1-4",
  "description": "A Catalog of Teas I Have Tried",
  "author": "Ken Sodemann <ken@ionic.io>",
  "private": true,
  "watch": {
    "lint": {
      "patterns": [
        "server",
        "src",
        "test/web/unit/specs",
        "test/server"
      ],
      "extensions": "js,jsx,vue"
    },
    "unitsrv": {
      "patterns": [
        "server",
        "test/server"
      ],
      "extensions": "js"
    },
    "unitweb": {
      "patterns": [
        "src",
        "test/web/unit/specs"
      ],
      "extensions": "js,jsx,vue"
    }
  },
  "scripts": {
    "build": "node build/build.js",
    "bump": "node scripts/bump-version.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "e2e": "node test/e2e/runner.js",
    "heroku-postbuild": "npm run build",
    "lint": "eslint --ext .js,.jsx,.vue server src test/web/unit/specs test/server",
    "release": "node scripts/release.js",
    "start": "node index.js",
    "test": "npm run lint && npm run unitweb && npm run unitsrv",
    "unitsrv": "mocha test/server/**/*.spec.js",
    "unitweb": "cross-env BABEL_ENV=test karma start test/web/unit/karma.conf.js --single-run",
    "watch": "npm-watch"
  },
  "dependencies": {
    "body-parser": "~1.18.3",
    "bootstrap": "~4.1.1",
    "bootstrap-vue": "~2.0.0-rc.11",
    "express": "~4.16.3",
    "font-awesome": "~4.7.0",
    "jsonwebtoken": "~8.3.0",
    "moment": "~2.22.2",
    "passport": "~0.4.0",
    "passport-local": "~1.0.0",
    "pg": "~7.4.3",
    "vee-validate": "~2.0.9",
    "vue": "~2.5.16",
    "vue-resource": "~1.5.1",
    "vue-router": "~3.0.1",
    "vuex": "~3.0.1"
  },
  "devDependencies": {
    "adj-noun": "~0.1.1",
    "autoprefixer": "~7.2.6",
    "babel-core": "~6.26.3",
    "babel-eslint": "~8.2.5",
    "babel-loader": "~7.1.5",
    "babel-plugin-istanbul": "~4.1.6",
    "babel-plugin-transform-runtime": "~6.22.0",
    "babel-preset-env": "~1.7.0",
    "babel-preset-stage-2": "~6.22.0",
    "babel-register": "~6.22.0",
    "chai": "~4.1.2",
    "chalk": "~2.4.1",
    "chromedriver": "~2.40.0",
    "connect-history-api-fallback": "~1.5.0",
    "conventional-changelog": "~1.1.24",
    "copy-webpack-plugin": "~4.5.2",
    "cross-env": "~5.2.0",
    "cross-spawn": "~5.1.0",
    "css-loader": "~0.28.11",
    "eslint": "~4.19.1",
    "eslint-config-standard": "~10.2.1",
    "eslint-friendly-formatter": "~3.0.0",
    "eslint-loader": "~1.7.1",
    "eslint-plugin-html": "~4.0.5",
    "eslint-plugin-import": "~2.13.0",
    "eslint-plugin-node": "~5.2.1",
    "eslint-plugin-promise": "~3.8.0",
    "eslint-plugin-standard": "~3.1.0",
    "eventsource-polyfill": "~0.9.6",
    "execa": "~0.8.0",
