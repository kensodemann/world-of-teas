{
  "name": "world-of-teas",
  "version": "0.0.1-2",
  "description": "A Catalog of Teas I Have Tried",
  "author": "Ken Sodemann <ken@ionic.io>",
  "private": true,
  "watch": {
    "lint": {
      "patterns": [
        "server",
        "src",
        "test/web/unit/specs",
        "test/server"
      ],
      "extensions": "js,jsx,vue"
    },
    "unitsrv": {
      "patterns": [
        "server",
        "test/server"
      ],
      "extensions": "js"
    },
    "unitweb": {
      "patterns": [
        "src",
        "test/web/unit/specs"
      ],
      "extensions": "js,jsx,vue"
    }
  },
  "scripts": {
    "bump": "node scripts/bump-version.js",
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "e2e": "node test/e2e/runner.js",
    "start": "node index.js",
    "build": "node build/build.js",
    "unitsrv": "mocha test/server/**/*.spec.js",
    "unitweb": "cross-env BABEL_ENV=test karma start test/web/unit/karma.conf.js --single-run",
    "test": "npm run lint && npm run unitweb && npm run unitsrv",
    "lint": "eslint --ext .js,.jsx,.vue server src test/web/unit/specs test/server",
    "heroku-postbuild": "npm run build",
    "watch": "npm-watch"
  },
  "dependencies": {
    "express": "^4.16.2",
    "moment": "^2.19.2",
    "pg": "^7.4.0",
    "vue": "^2.5.8",
    "vue-resource": "^1.3.4",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "adj-noun": "^0.1.1",
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^4.1.2",
    "chalk": "^2.3.0",
    "chromedriver": "^2.33.2",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "^4.2.3",
    "cross-env": "^5.1.1",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.0",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.3",
    "inject-loader": "^3.0.0",
    "karma": "^1.4.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sinon-chai": "^1.3.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.6",
    "mocha": "^4.0.1",
    "nightwatch": "^0.9.16",
    "node-sass": "^4.7.2",
    "npm-watch": "^0.3.0",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "phantomjs-prebuilt": "^2.1.16",
    "portfinder": "^1.0.13",
    "proxyquire": "^1.8.0",
