    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1"
  },
  "engines": {
    "node": "^8.6.0",
    "npm": "^5.5.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
